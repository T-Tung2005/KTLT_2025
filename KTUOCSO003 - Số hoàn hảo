#include <bits/stdc++.h>
using namespace std;
const int mod=1e9+7;
bool check(int n)
{
    if(n<=1) return 0;
    long long tong=1;
    for(int i=2;i<=sqrt(n);i++)
    {
        if(n%i==0)
        {
            tong+=i;
            if(i!=n/i) tong+=n/i;
        }
    }
    return tong==n;
}
int main()
{
    int q;
    cin>>q;
    while(q--)
    {
        int n;
        cin>>n;
        if(check(n))
            {
                cout<<1<<endl;
            }else cout<<0<<endl;
    }
}
