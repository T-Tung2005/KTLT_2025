#include <bits/stdc++.h>
using namespace std;
int timkiem(int a[],int n,int x)
{
    int l=0;int r=n-1;
    int res=-1;
    while(l<=r)
    {
        int mid=(l+r)/2;
        if(a[mid]==x)
        {
            res=mid;
            r=mid-1;
        }else if(a[mid]<x)
        {
            l=mid+1;
        }else{
           r=mid-1;
        }
    }
    return res;
}
int main()
{
   int q;
   cin>>q;
   while(q--)
   {
       int n;
       cin>>n;
       int a[n];
       for(int i=0;i<n;i++)
       {
           cin>>a[i];
       }
       sort(a,a+n);
       int min1=a[0];
       int max1=a[n-1];
       int dem=0;
       for(int i=min1;i<=max1;i++)
       {
           if(timkiem(a,n,i)==-1)
           {
               dem++;
           }
       }
       cout<<dem<<endl;
   }
}
