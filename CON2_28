#include <bits/stdc++.h>
using namespace std;
int a[100];
int x[100];
int n,k;
using ll= long long;
bool check;
void try1(int i,int bd,int sum)
{
    for(int j=bd;j<=n;j++)
    {
        x[i]=a[j];
        sum+=a[j];
        if(sum==k)
        {
            check=true;
            cout<<"[";
            for(int h=1;h<=i;h++)
            {
                cout<<x[h];
                if(h!=i)
                {
                    cout<<" ";
                }
            }
            cout<<"]";
        }else if(sum<k)
        try1(i+1,j,sum);
        sum-=a[j];
    }
}
int main()
{
   int q;
   cin>>q;
   while(q--)
   {
       cin>>n>>k;
       for(int i=1;i<=n;i++)
       {
           cin>>a[i];
       }
       check=false;
       try1(1,1,0);
       if(!check) cout<<-1;
       cout<<endl;
   }
}
