#include <bits/stdc++.h>
using namespace std;
bool fibo(long long n)
{
    long long f[100];
    f[0]=0,f[1]=1;
    for(int i=2;i<=92;i++)
    {
        f[i]=f[i-2]+f[i-1];
    }
    for(int i=0;i<=92;i++)
    {
        if(f[i]==n)
            return 1;
    }
    return 0;
}
int main() {
    int q;
    cin>>q;
    while(q--)
    {
        long long n;
        cin>>n;
        while(1)
        {
            int m=n;
            n++;
            if(fibo(n)==0)
            {
                if(n>m)
                {
                    cout<<n<<endl;
                    break;
                }
            }else{
              continue;
            }
        }
    }
}
