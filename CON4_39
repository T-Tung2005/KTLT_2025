#include <bits/stdc++.h>
using namespace std;
int timkiem(int a[],int n,int x)
{
    int l=0;int r=n-1;
    int res=-1;
    while(l<=r)
    {
        int mid=(l+r)/2;
        if(a[mid]==x)
        {
            res=mid;
            r=mid-1;
        }else if(a[mid]<x)
        {
            l=mid+1;
        }else{
         r=mid-1;
        }
    }
    return res;
}
int main() {
    int q;
    cin>>q;
    while(q--)
    {
      int n;
      cin>>n;
      int a[n];
      int b[n-1];
      for(int i=0;i<n;i++)
      {
          cin>>a[i];
      }
      for(int j=0;j<n-1;j++)
      {
          cin>>b[j];
      }
      for(int h=0;h<n;h++)
      {
          if(timkiem(b,n-1,a[h])==-1)
          {
              cout<<h+1<<endl;
          }
      }
    }
}
