#include <bits/stdc++.h>
using namespace std;
bool check(long long n)
{
    int ans=0;
    long long m=n;
    for(int i=2;i<=sqrt(n);i++)
    {
       while(n%i==0)
       {
           ans=i;
           n/=i;
       }
    }
    if(n>1) ans=n;
    if(ans > sqrt(m))
        return 1;
    return 0;
}
int main()
{
   int q;
   cin>>q;
   while(q--)
   {
       int n;
       cin>>n;
       int ans=0;
       for(int i=n;i>=1;i--)
       {
           if(check(i))
           {
               ans=i;
               break;
           }
       }
       cout<<ans<<endl;
   }
}
