#include <bits/stdc++.h>
using namespace std;
using ll= long long;
int main(){
    int q;
    cin>>q;
    while(q--)
    {
        string s1,s2;
        cin>>s1>>s2;
        long long sum1=0,sum2=0;
        for(int i=0;i<16;i++)
        {
            sum1=(sum1*2)+(s1[i]-'0');
            sum2=(sum2*2)+(s2[i]-'0');
        }
        long long ans=1ll*sum1*sum2;
        cout<<ans<<endl;
    }
}
