#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
vector<int>v;
int n,k,s;
void sang(ll L, ll  R){
	int prime[R - L + 1];
	for(ll i = L; i <= R; i++){
		prime[i - L] = 1;
	}
	for(ll i = 2; i <= sqrt(R); i++){
		for(ll j = max(i * i, (L + i - 1) / i * i); j <= R; j += i){
			prime[j - L] = 0;
		}
	}
	for(ll i = max(2ll, L); i <= R; i++){
		if(prime[i - L]){
			v.push_back(i);
		}
	}
}
vector<vector<int>>v1;
void try1(int index,int sum,vector<int>tmp)
{
    for(int j=index;j< v.size();j++)
    {
        sum+=v[j];
        tmp.push_back(v[j]);
        if(sum==s && tmp.size()==k)
        {
            v1.push_back(tmp);
        }else if(sum<s)
        {
            try1(j+1,sum,tmp);
        }
        sum-=v[j];
        tmp.pop_back();
    }
}
int main()
{
   cin>>n>>k>>s;
   sang(n+1,s);
   vector<int>h;
   try1(0,0,h);
   if(v1.size()==0) cout<<"-1"<<endl;
   for(auto x : v1)
   {
       for(auto y:x)
       {
           cout<<y<<" ";
       }
       cout<<endl;
   }
}
