#include <bits/stdc++.h>
using namespace std;
int main() {
     int q;
     cin>>q;
     while(q--)
     {
         int n,m;
         cin>>n>>m;
         int h=n+m;
         int a[h];
         for(int i=0;i<h;i++)
         {
             cin>>a[i];
         }
         sort(a,a+h);
         for(int x : a)
         {
             cout<<x<<" ";
         }
         cout<<endl;
     }
}
