#include <bits/stdc++.h>
using namespace std;
long long check(long long n)
{
    long long res = -1;
    while(n % 2 == 0) {
        res = 2;
        n /= 2;
    }
    for(long long i = 3; i*i <= n; i += 2)
    {
        while(n % i == 0)
        {
            res = i;
            n /= i;
        }
    }
    if(n > 1) res = n;
    return res;
}
int main() {
    int q;
    cin>>q;
    while(q--)
    {
  long long n;
  cin>>n;
  cout<<check(n)<<endl;
    }
}
