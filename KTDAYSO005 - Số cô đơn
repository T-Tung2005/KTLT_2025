#include <bits/stdc++.h>
using namespace std;
int main()
{
    int q;
    cin>>q;
    while(q--)
    {
        int n;
        cin>>n;
        int a[n];
        map<int,int>mp;
        for(int i=0;i<n;i++)
        {
            cin>>a[i];
            mp[a[i]]++;
        }
        int dem=0;
        for(int x : a)
        {
            if(mp[x]==1)
            {
                dem++;
            }
        }
        cout<<dem<<endl;
    }
}
