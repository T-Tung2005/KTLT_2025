#include<bits/stdc++.h>
using namespace std;
long long binpow(long long a,long long n)
{
    if(n==0) return 1;
    long long x = binpow(a,n/2);
    if(n%2==0)
    {
        return x*x;
    }else{
      return x*x*a;
     }
}
int main()
{
   int q;
   cin>>q;
   while(q--)
   {
       string s;
       cin>>s;
       long long tong=0;
       int h=s.size()-1;
       for(int i=0;i<s.size();i++)
       {
           if(s[i]=='1')
           {
              tong+=binpow(2,h);
           }
           h--;
       }
       cout<<tong<<endl;
   }
}
