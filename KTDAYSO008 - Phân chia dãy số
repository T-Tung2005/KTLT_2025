#include<bits/stdc++.h>
using namespace std;
int main()
{
    int q;
    cin>>q;
    while(q--)
    {
        int n;
        cin>>n;
        int a[n];
        long long tong=0;
        for(int i=0;i<n;i++)
        {
            cin>>a[i];
            tong+=a[i];
        }
        long long cut=tong/2;
        vector<int>dp(cut+1,0);
        dp[0]=1;
        for(int i=0;i<n;i++)
        {
            for(int j=cut;j>=a[i];j--)
            {
                if(dp[j-a[i]])
                {
                    dp[j]=1;
                }
            }
        }
        for(int i=cut;i>=0;i--)
        {
            if(dp[i])
            {
                cout<<tong-2*i<<endl;
                break;
            }
        }
    }
}
