#include <bits/stdc++.h>
using namespace std;
const int mod=1e9+7;
int main()
{
    int q;
    cin>>q;
    while(q--)
    {
        int n;
        cin>>n;
        vector<int>dp(n+1,0);
        dp[0]=1;
        for(int i=1;i<n;i++)
        {
            for(int j=1;j<=n;j++)
            {
                if(j>=i)
                 dp[j]=(dp[j]+dp[j-i])%mod;
            }
        }
        cout<<dp[n]<<endl;
    }
}
