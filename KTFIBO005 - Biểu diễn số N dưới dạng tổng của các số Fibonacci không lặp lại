#include <bits/stdc++.h>
using namespace std;
vector<int>v;
void fibo(long long n)
{
    v.clear();
    int f1,f2,fn;
    f1=1;f2=1;
    v.push_back(1);
    for(int i=2;i<=n;i++)
    {
        fn=f1+f2;
        if(fn>n) break;
        v.push_back(fn);
        f1=f2;
        f2=fn;
    }
}
int main() {
    int q;
    cin>>q;
    while(q--)
    {
        long long n;
        cin>>n;
        fibo(n);
        vector<int>dp(n+1,0);
        dp[0]=1;
        for(auto x : v)
        {
           for(int j=n;j>=x;j--)
           {
              dp[j]=dp[j]+dp[j-x];
           }
        }
       cout<<dp[n]<<endl;
    }
}
