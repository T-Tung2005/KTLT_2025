#include <bits/stdc++.h>
using namespace std;
int ok;
string s;
int n;
void kt()
{
    s=string(n,'A');
}
void sinh()
{
    int i=s.size()-1;
    while(i>=0&&s[i]=='B')
    {
        s[i]='A';
        i--;
    }
    if(i<0) ok=0;
    else
    {
        s[i]='B';
    }
}
int main()
{
    int q;
    cin>>q;
    while(q--)
    {
        cin>>n;
        ok=1;
        kt();
        vector<string>v;
        while(ok)
        {
            string tmp;
            for(int i=0;i<n;i++)
            {
                tmp+=s[i];
            }
            v.push_back(tmp);
            sinh();
        }
        for(auto x : v)
        {
            cout<<x<<" ";
        }
        cout<<endl;
    }
}
