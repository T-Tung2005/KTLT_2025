#include <bits/stdc++.h>
using namespace std;
int main()
{
    int q;
    cin>>q;
    while(q--)
    {
   int n,s;
   cin>>n>>s;
   int a[n];
   for(int i=0;i<n;i++)
   {
       cin>>a[i];
   }
   vector<int>dp(s+1,1e9);
   dp[0]=0;
   for(int i=0;i<n;i++)
   {
       for(int j=s;j>=a[i];j--)
       {
               dp[j]=min(dp[j],dp[j-a[i]]+1);
       }
   }
   if(dp[s]==1e9)
   {
       cout<<-1<<endl;
   }else
    cout<<dp[s]<<endl;
}
}
