#include <bits/stdc++.h>
using namespace std;
bool cmp(string a, string b) {
    return a + b > b + a;
}
int main() {
    int q;
    cin >> q;
    while (q--) {
        int n;
        cin >> n;
        vector<string> a(n);
        for (int i = 0; i < n; i++) cin >> a[i];
        sort(a.begin(), a.end(), cmp);
        string ans = "";
        for (auto &x : a) ans += x;
        //if (ans[0] == '0') ans = "0";
        cout << ans << endl;
    }
    return 0;
}
