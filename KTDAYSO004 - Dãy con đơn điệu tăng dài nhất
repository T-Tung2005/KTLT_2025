#include <bits/stdc++.h>
using namespace std;
int main()
{
    int q;
    cin>>q;
    while(q--)
    {
   int n;
   cin>>n;
   int a[n];
   long long f[10001];
   for(int i=1;i<=n;i++)
   {
       cin>>a[i];
   }
   for(int i=1;i<=n;i++)
   {
       f[i]=1;
       for(int j=1;j<=i;j++)
       {
           if(a[i]> a[j])
           {
               f[i]=max(f[i],f[j]+1);
           }
       }
   }
   sort(f,f+n,greater<int>());
   cout<<f[0]<<endl;
    }
}
