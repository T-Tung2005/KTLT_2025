#include <bits/stdc++.h>
using namespace std;
long long check(long long n)
{
    long long res = -1;
    while(n % 2 == 0) {
        res = 2;
        n /= 2;
    }
    for(long long i = 3; i*i <= n; i += 2)
    {
        while(n % i == 0)
        {
            res = i;
            n /= i;
        }
    }
    if(n > 1) res = n;
    return res;
}
int main() {
    int q;
    cin>>q;
    while(q--)
    {
     int l,r;
     cin>>l>>r;
     long long d=0;
     for(int i=l;i<=r;i++)
     {
         d=max(d,check(i));
     }
     cout<<d<<endl;
    }
}
