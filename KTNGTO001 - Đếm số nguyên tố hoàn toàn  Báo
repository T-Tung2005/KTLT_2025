#include<bits/stdc++.h>
using namespace std;
bool checknt(long long n)
{
    if(n<2) return 0;
    for(int i=2;i<=sqrt(n);i++)
    {
        if(n%i==0)
        {
            return 0;
        }
    }
    return 1;
}
bool check(long long n)
{
  if(n==0) return 0;
  while(n!=0)
  {
      int r=n%10;
      if(r!=2&&r!=3&&r!=5&&r!=7)
      {
         return 0;
      }
      n/=10;
  }
  return 1;
}
int main()
{
    int q;
    cin>>q;
    while(q--)
    {
        int n;
        cin>>n;
        int dem=0;
        for(int i=0;i<=n;i++)
        {
            if(check(i)&&checknt(i))
            {
                dem++;
            }
        }
        cout<<dem<<endl;
    }
}
