#include <bits/stdc++.h>
using namespace std;
using ll= long long;
int main(){
    int q;
    cin>>q;
    while(q--)
    {
    int n;
    cin>>n;
    int a[n];
    int res[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    stack<int>st;
    for(int i=n-1;i>=0;i--)
    {
        while(!st.empty()&&st.top()<=a[i])
        {
            st.pop();
        }
        if(st.empty()) res[i]=-1;
        else res[i]=st.top();
        st.push(a[i]);
    }
    for(int i=0;i<n;i++)
    {
        cout<<res[i]<<" ";
    }
    cout<<endl;
}
}
