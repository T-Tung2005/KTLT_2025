#include <bits/stdc++.h>
using namespace std;
int a[100][100];
int dp[105][105];
int main()
{
    int q;
    cin>>q;
    while(q--)
    {
    int n,m;
    cin>>n>>m;
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=m;j++)
        {
            cin>>a[i][j];
            dp[i][j]=0;
        }
    }
    for(int j=1;j<=m;j++)
    {
        for(int i=1;i<=n;i++)
        {
            int truoc=dp[i][j-1];
            if(i>1) truoc=max(truoc,dp[i-1][j-1]);
            if(i<n) truoc=max(truoc,dp[i+1][j-1]);
             dp[i][j]=truoc+a[i][j];
        }
    }
    int tmp=-1e9;
    for(int i=1;i<=n;i++)
    {
        tmp=max(tmp,dp[i][m]);
    }
    cout<<tmp<<endl;
    }
}
