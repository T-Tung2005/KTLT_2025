#include <bits/stdc++.h>
using namespace std;
int main()
{
    int q;
    cin>>q;
    while(q--)
    {
    long long n;
    cin>>n;
    vector<long long>a(n);
    for(long long i=0;i<n;i++)
    {
        cin>>a[i];
    }
    vector<long long>dp(n);
    dp[0]=a[0];
    long long res=dp[0];
    for(long long i=1;i<n;i++)
    {
        dp[i]=max(a[i],dp[i-1]+a[i]);
        res=max(res,dp[i]);
    }
    cout<<res<<endl;
    }
}
